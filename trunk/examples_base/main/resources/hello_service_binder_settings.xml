<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <!-- Server base settings -->
    <entry key="defaultLanguage">sv</entry>
    <entry key="defaultCountry">SE</entry>

    <!-- Database settings -->
    <entry key="database.server">localhost</entry>
    <entry key="database.port">3306</entry>
    <entry key="database.name">test</entry>
    <entry key="database.invocationUser">invocationUser</entry>
    <entry key="database.password">password</entry>

    <!-- Web settings -->
    <entry key="serverBaseUrl">http://localhost:8080/</entry>

</properties>